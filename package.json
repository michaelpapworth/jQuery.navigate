    }
  ],
  "bugs": {
    "url": "https://github.com/michaelpapworth/jquery.navigate/issues"
  },
  "homepage": "https://github.com/michaelpapworth/jquery.navigate",
  "scripts": {
    "build": "npm install && grunt",
    "start": "grunt watch",
    "test": "grunt"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
